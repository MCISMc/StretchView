{
   "background": {
      "service_worker": "js/background.js"
   },
   "action": {
      "default_icon": {
         "128": "images/logo-128.png",
         "16": "images/logo-32.png",
         "32": "images/logo-32.png",
         "48": "images/logo-48.png"
      },
      "default_popup": "html/popup.html",
      "default_title": "StretchView"
   },
   "content_scripts": [
      {
         "js": [
            "js/jquery/fullscreen/jquery.fullscreen.js",
            "js/jquery/hotkeys/jquery.hotkeys.js",
            "js/jquery/jquery-2.1.3/jquery-2.1.3.min.js",
            "js/StretchView.js"
         ],
         "matches": [
            "https://*.youtube.com/*",
            "https://*.primevideo.com/*",
            "https://*.twitch.tv/*",
            "https://*.hotstar.com/*",
            "https://*.sonyliv.com/*",
            "https://*.netflix.com/*",
            "https://*.erosnow.com/*",
            "https://*.voot.com/*",
            "https://*.zee5.com/*",
            "https://*.altbalaji.com/*",
            "https://*.jiocinema.com/*",
            "https://*.facebook.com/*",
            "https://*.twitter.com/*",
            "https://*.instagram.com/*",
            "https://*.airtelxstream.in/*",
            "https://*.mxplayer.in/*"
         ],
         "run_at": "document_start"
      }
   ],
   "default_locale": "en",
   "description": "Remove black bars from streaming videos. Control aspect ratio, use PiP mode, and enhance video playback on popular platforms.",
   "icons": {
      "128": "images/logo-128.png",
      "16": "images/logo-32.png",
      "32": "images/logo-32.png",
      "48": "images/logo-48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "StretchView",
   "permissions": [
      "activeTab",
      "storage",
      "scripting"
   ],
   "short_name": "StretchView",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "6.1.0"
}